{% for s in stages %}
    <h2>{{ s.stage.name }}</h2>
    <div class="row">
        <div class="col-xs-12 col-md-7">
            {% include 'WcGameBundle:Default/_p:games.html.twig' with {'games': s.stage.games, 'stage': s.stage.name} %}
        </div>
        <div class="col-xs-12 col-md-5">
            {% include 'WcGameBundle:Default/_p:standing.html.twig' with {'standings': s.standings } %}
        </div>
    </div>
{% endfor %}