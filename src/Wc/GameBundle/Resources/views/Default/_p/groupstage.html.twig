{% for s in stages %}
    <h2><i class="glyphicon glyphicon-minus"></i> {{ s.stage.name }}</h2>
    <div class="row">
        <div class="col-xs-6">
            {% include 'WcGameBundle:Default/_p:games.html.twig' with {'games': s.stage.games} %}
        </div>
        <div class="col-xs-6">
            {% include 'WcGameBundle:Default/_p:standing.html.twig' with {'standings': s.standings } %}
        </div>
    </div>
{% endfor %}